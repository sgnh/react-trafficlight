{
  "name": "react-trafficlight",
  "version": "4.0.1",
  "description": "Traffic light component for React.",
  "repository": "sgnh/react-trafficlight",
  "license": "MIT",
  "author": "Steffen Hansen",
  "keywords": [
    "react",
    "component",
    "trafficlight",
    "traffic",
    "light"
  ],
  "scripts": {
    "build": "npm run build:babel && npm run build:copy-files",
    "build:babel": "babel src -d build/lib",
    "build:copy-files": "babel-node ./scripts/copy-files.js",
    "dev": "babel -w src -d build/lib",
    "clean": "git clean -fdX ."
  },
  "peerDependencies": {
    "prop-types": "^15.5.4",
    "react": "^15.0.0"
  },
  "devDependencies": {
    "babel-cli": "^6.10.1",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.5",
    "babel-preset-env": "^1.5.1",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-0": "^6.5.0",
    "eslint": "^3.13.1",
    "eslint-config-airbnb": "^15.0.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^5.0.3",
    "eslint-plugin-react": "^7.0.1",
    "fs-extra": "^3.0.1",
    "prop-types": "^15.5.4",
    "react": "^15.4.2"
  },
  "private": true
}
